{{! Pull default.hbs markup - DO NOT REMOVE }}
	{{!< default}}


{{! Page }}
	<main class="clearfix" role="main">
		{{#post}}
			<article class="{{post_class}} odd last">
				<div class="container clearfix animate-in" data-anim-type="fade-in-up">
					<h1 class="post-title bm-large"><a href="{{url}}" target="_self">{{{title}}}</a></h1>
				
					<div class="article-body">{{content}}</div>
				</div>
			</article>
		{{/post}}
	</main>