{{! Pull default.hbs markup - DO NOT REMOVE }}
	{{!< default}}


{{! Entry Feed - Tag Filter }}
	<main class="clearfix" role="main">

		{{! Tag }}
			<div class="container bm-remove">
				<h4 class="bm-larger tm-larger text-right">Filter by tag: <em>"{{tag.name}}"</em></h4>
			</div>

			<hr class="bm-remove separator" />


		{{! Post Loop - partials/post-loop.hbs }}
			{{> "post-loop"}}


		{{!! Pagination - partials/pagination.hbs }}
			{{pagination}}
			
	</main>